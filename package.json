{
  "name": "x-hd-wallet-api-crypto",
  "version": "0.0.5",
  "description": "A Typescript implementation of BIP-32 Ed25519 for Algorand ARC-52.",
  "main": "./dist/x.hd.wallet.api.crypto.cjs.js",
  "module": "./dist/x.hd.wallet.api.crypto.esm.js",
  "types": "./dist/x.hd.wallet.api.crypto.d.ts",
  "scripts": {
    "test": "jest --verbose",
    "test:cov": "jest --coverage",
    "build": "rollup --config"
  },
  "keywords": [],
  "author": "Algorand Foundation",
  "license": "Apache-2.0",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.5",
    "@types/libsodium-wrappers-sumo": "^0.7.7",
    "@types/node": "^20.7.1",
    "rollup": "^4.21.3",
    "ts-jest": "^29.1.1",
    "tslib": "^2.7.0",
    "tweetnacl": "^1.0.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "algo-msgpack-with-bigint": "^2.1.1",
    "bip32-ed25519": "^0.0.4",
    "bip39": "^3.1.0",
    "bn.js": "^5.2.1",
    "hi-base32": "^0.5.1",
    "jest": "^29.7.0",
    "js-sha512": "^0.8.0",
    "libsodium-wrappers-sumo": "^0.7.13",
    "ts-custom-error": "^3.2.0",
    "ts-log": "^2.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../test/coverage",
    "collectCoverageFrom": [
      "**/!(*.module|*.interface|main|repl|exception.filter|logging.interceptor).{ts,js}"
    ],
    "coveragePathIgnorePatterns": [
      "/src/migration/"
    ],
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acfunk/xHD-Wallet-API-ts.git"
  }
}
